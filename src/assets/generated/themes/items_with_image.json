                  {
                    "and": [
                      "seasonal~.*spring.*",
                      "_now:date~....-(03|04|05)-.."
                    ]
                  },
                  {
                    "and": [
                      "seasonal~.*summer.*",
                      "_now:date~....-(06|07|08)-.."
                    ]
                  },
                  {
                    "and": [
                      "seasonal~.*autumn.*",
                      "_now:date~....-(09|10|11)-.."
                    ]
                  }
                ]
              }
            ]
          }
        },
        {
