              "human": {
                "en": " megawatts",
                "nl": " megawatt",
                "fr": " megawatts",
                "de": " Megawatt",
                "eo": " megavatoj",
                "it": " megawatt"
              }
            },
            {
              "canonicalDenomination": "kW",
              "alternativeDenomination": [
                "kilowatts",
                "kilowatt"
              ],
              "human": {
                "en": " kilowatts",
                "nl": " kilowatt",
                "fr": " kilowatts",
                "de": " Kilowatt",
                "eo": " kilovatoj",
                "it": " kilowatt",
                "nb_NO": " kilowatt"
              }
            },
            {
              "canonicalDenomination": "W",
              "alternativeDenomination": [
                "watts",
                "watt"
              ],
              "human": {
                "en": " watts",
