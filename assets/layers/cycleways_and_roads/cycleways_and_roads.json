                    "hideInAnswer": true
                },
                {
                    "if": "lit=24/7",
                    "then": {
                        "en": "This road is lit 24/7",
                        "nl": "Deze weg is 24/7 verlicht"
                    }
                }
            ],
            "id": "is lit?"
        },
        {
