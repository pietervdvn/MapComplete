      "nl": "Speeltuin",
      "en": "Playground",
      "it": "Parco giochi",
      "ru": "Детская площадка",
      "de": "Spielplatz",
      "fr": "Aire de jeu"
    },
    "mappings": [
      {
        "if": "name~*",
        "then": {
          "nl": "Speeltuin <i>{name}</i>",
          "en": "Playground <i>{name}</i>",
          "it": "Parco giochi <i>{name}</i>",
          "ru": "Детская площадка <i>{name}</i>",
          "de": "Spielplatz <i>{name}</i>",
          "fr": "Aire de jeu <i>{name}</i>"
        }
      }
    ]
  },
