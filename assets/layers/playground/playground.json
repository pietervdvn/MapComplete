  "presets": [
    {
      "tags": [
        "leisure=playground"
      ],
      "title": {
        "nl": "een speeltuin",
        "en": "a playground",
        "ru": "Детская площадка",
        "fr": "une terrain de jeux",
        "it": "una campetto",
        "de": "eine spielplatz"
      }
    }
  ],
  "deletion": {
    "softDeletionTags": {
      "and": [
        "disused:leisure=playground",
        "leisure="
      ]
    }
  },
  "mapRendering": [
    {
      "icon": {
        "render": "./assets/themes/playgrounds/playground.svg"
      },
      "iconBadges": [
        {
          "if": {
            "and": [
              "opening_hours!=24/7",
              "opening_hours~*"
            ]
          },
          "then": "isOpen"
        }
      ],
      "iconSize": {
        "render": "40,40,center",
        "mappings": [
          {
            "if": "id~node/.*",
            "then": "40,40,center"
          },
          {
            "if": "_size_classification=small",
            "then": "25,25,center"
          },
          {
            "if": "_size_classification=medium",
            "then": "40,40,center"
          },
          {
            "if": "_size_classification=large",
            "then": "60,60,center"
          }
        ]
      },
      "location": [
        "point",
        "centroid"
      ]
    },
    {
      "color": {
        "render": "#5dbaa9"
      },
      "width": {
        "render": "1"
      }
    }
  ]
}