    {
      "id": "not_opened_by",
      "options": [
        {
          "osmTags": "_first_user!~i~.*{search}.*",
          "fields": [
            {
              "name": "search"
            }
          ],
          "question": {
            "en": "<b>Not</b> opened by contributor {search}",
            "nl": "<b>Niet</b> geopend door bijdrager {search}",
            "de": "<b>Nicht</b> geöffnet von Beitragendem {search}",
