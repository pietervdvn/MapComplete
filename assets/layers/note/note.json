        {
          "osmTags": "_last_user!~i~.*{search}.*",
          "fields": [
            {
              "name": "search"
            }
          ],
          "question": {
            "en": "Not edited as last by {search}",
            "de": "Nicht zuletzt bearbeitet von {search}",
